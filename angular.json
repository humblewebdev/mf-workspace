{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "punch-widget": {
      "projectType": "library",
      "root": "projects/punch-widget",
      "sourceRoot": "projects/punch-widget/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/punch-widget/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/punch-widget/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/punch-widget/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/punch-widget/src/test.ts",
            "tsConfig": "projects/punch-widget/tsconfig.spec.json",
            "karmaConfig": "projects/punch-widget/karma.conf.js"
          }
        }
      }
    },
    "punch-stats-widget": {
