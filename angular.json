            },
            "development": {
              "tsConfig": "projects/punch-stats-widget/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/punch-stats-widget/src/test.ts",
            "tsConfig": "projects/punch-stats-widget/tsconfig.spec.json",
            "karmaConfig": "projects/punch-stats-widget/karma.conf.js"
          }
        }
      }
    },
    "daily-punch-activity-widget": {
      "projectType": "library",
      "root": "projects/daily-punch-activity-widget",
      "sourceRoot": "projects/daily-punch-activity-widget/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/daily-punch-activity-widget/ng-package.json"
          },
          "configurations": {
            "production": {
